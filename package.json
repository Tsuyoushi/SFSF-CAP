{
  "name": "sfsf-projman",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^2.7.0",
    "@sap/cds": ">=5",
    "@sap/xsenv": "^3.3.2",
    "@sap/xssec": "^3.2.13",
    "express": "^4",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "sqlite3": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "ECEmployeeProfile": {
        "kind": "odata-v2",
        "model": "srv\\external\\ECEmployeeProfile",
        "credentials": {
          "destination": "SFSF",
          "path": "/odata/v2/Background_SpecialAssign"
        }
      },
      "PLTUserManagement": {
        "kind": "odata-v2",
        "model": "srv\\external\\PLTUserManagement",
        "credentials": {
          "destination": "SFSF",
          "path": "/odata/v2/User"
        }
      }
    }
  }
}
